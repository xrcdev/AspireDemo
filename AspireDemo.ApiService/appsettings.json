{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Consul": {
    "ServiceName": "apiservice",
    "PathPrefix": "/weather",
    "PreferredNetworks": "172",
    "Tags": [ "weather" ]
  },
  "OpenTelemetry": {
    "Exporters": {
      "Otlp": {
        "Enabled": "true"
      },
      "Console": {
        "Enabled": "false"
      },
      "InMemory": {
        "Enabled": "false"
      },
      "Prometheus": {
        "Enabled": "false",
        "Port": "9464"
      }
    }
  }
}
