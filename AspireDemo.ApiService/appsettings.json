{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Consul": {
    "Address": "http://localhost:8500",
    "ServiceName": "apiservice",
    "PathPrefix": "/api",
    "Weight": 1,
    "Scheme": "http",
    "Protocol": "http",
    "HealthCheckPath": "/health",
    "HealthCheckIntervalSeconds": 10,
    "HealthCheckTimeoutSeconds": 5,
    "DeregisterCriticalServiceAfterSeconds": 30
  }
}
